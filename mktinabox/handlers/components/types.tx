/*
	Component types
*/

ItemDescription:
	/[a-zA-Z]+(\s[a-zA-Z0-9\+]+)*(\s)/
;

Float:
	/\d{1,4}(.\d{3})*(\,\d{2})/
;

Percentage:
	/\d{1,2}\%/
;

Name:
  /([a-zA-Z0-9]|[- @\/\.,#&!])*/
;

Fecha:
	/\d{1,2}\/\d{1,2}\/\d{4}/
;

Hora:
	/\d{1,2}:\d{1,2}:\d{1,2}/	
;

MeanOfPayment:
	/[a-zA-Z]+(\s[a-zA-Z]+)*(\s)/
;

Text:
    /([\s\w\d\*\-@\/\.,#&!]|[\s]|[\n])*$/
;